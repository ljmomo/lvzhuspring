<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--本地数据库驱动-->
    <classPathEntry location="/Users/lvzhu/.m2/repository/mysql/mysql-connector-java/5.1.46/mysql-connector-java-5.1.46.jar"/>
    <context id="MysqlTables" targetRuntime="MyBatis3">
        <!--去除注释 -->
        <commentGenerator type="com.lv.lvzhudemo.code.MyCommentGenerator">
            <property name="author" value="lv zhu"/>
            <property name="dateFormat" value="yyyy/MM/dd"/>
        </commentGenerator>


        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/ebiz_spotlight?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false&amp;useJDBCCompliantTimezoneShift=true&amp;useLegacyDatetimeCode=false&amp;serverTimezone=UTC"
                        userId="root"
                        password="123456">
            <property name="useInformationSchema" value="true" />
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--实体存放位置-->
        <javaModelGenerator targetPackage="com.youzan.ebiz.spotlight.basic.dataobject"
                            targetProject="/Users/lvzhu/youzan/spotlight-basic/spotlight-basic-dal/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--XML存放位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="/Users/lvzhu/youzan/spotlight-basic/spotlight-basic-dal/src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!--Mapper存放位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.youzan.ebiz.spotlight.basic.dao"
                             targetProject="/Users/lvzhu/youzan/spotlight-basic/spotlight-basic-dal/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


        <!--需要生成的表-->
        <table schema="spt_shop_material_center" tableName="spt_shop_material_center"
               domainObjectName="ShopMaterialCenterDO"
               mapperName="ShopMaterialCenterDao"
               enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false" >
            <property name="useActualColumnNames" value="false"/>

        </table>


    </context>
</generatorConfiguration>